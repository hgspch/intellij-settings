<application>
  <component name="DaemonCodeAnalyzerSettings">
    <option name="SHOW_METHOD_SEPARATORS" value="true" />
  </component>
  <component name="DefaultFont">
    <option name="FONT_SIZE" value="17" />
    <option name="FONT_FAMILY" value="Inconsolata" />
    <option name="USE_LIGATURES" value="true" />
  </component>
  <component name="EditorSettings">
    <option name="USE_SOFT_WRAPS" value="MAIN_EDITOR" />
    <option name="USE_CUSTOM_SOFT_WRAP_INDENT" value="true" />
    <option name="STRIP_TRAILING_SPACES" value="Whole" />
  </component>
  <component name="LiveEditOptions" chromeUpdateApp="true" chromeUpdateOnChanges="true" nodeUpdateOnChanges="true" restartIfHotSwapFailed="true">
    <option name="elementHighlighting" value="true" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\b(todo|fixme)\b.*" />
    <pattern pattern="hug" />
    <filter name="hug">
      <pattern index="1" />
    </filter>
  </component>
</application>